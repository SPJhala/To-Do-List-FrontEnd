<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="412.0" prefWidth="426.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mainframe.todolistfrontend.ToDoListUI">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="displayButton" mnemonicParsing="false" onAction="#onDisplayButtonClick" text="Show List" />
   <ImageView fitHeight="25.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
   <Button fx:id="addActivityButton" mnemonicParsing="false" onAction="#onaddActivityButtonNewWindow" text="Add Activity" />

    <Label fx:id="welcomeText" />
   <Button fx:id="removeActivityButton" mnemonicParsing="false" onAction="#onRemoveActivityButtonNewWindow" text="Remove Activity" />
</VBox>
